{
  "name": "angular2-aot-webpack",
  "version": "1.0.0",
  "description": "Angular 2 AOT (Ahead Of Time) compilation example with Webpack",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server",
    "build": "npm run ngc && npm run webpack-prod",
    "explorer": "npm run ngc && npm run webpack-prod-src-map && source-map-explorer ./dist/main.bundle.js",
    "clean": "rimraf dist ngfactory src/**/*.ngfactory.ts src/**/*.shim.ts",
    "ngc": "ngc -p ./tsconfig.aot.json",
    "prebuild": "npm run clean",
    "webpack-prod-src-map": "cross-env NODE_ENV=production WEBPACK_KEEP_SRC_MAP=true webpack",
    "webpack-prod": "cross-env NODE_ENV=production webpack"
  },
  "keywords": [
    "angular2",
    "aot",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gouravcoder/angular2-aot-webpack.git"
  },
  "author": {
    "name": "Gourav Sharma",
    "email": "gouravsharma2006@gmail.com"
  },
  "license": "ISC",
  "dependencies": {
    "@angular/common": "2.1.2",
    "@angular/compiler": "2.1.2",
    "@angular/compiler-cli": "2.1.2",
    "@angular/core": "2.1.2",
    "@angular/forms": "^2.1.2",
    "@angular/http": "^2.1.2",
    "@angular/platform-browser": "2.1.2",
    "@angular/platform-browser-dynamic": "2.1.2",
    "@angular/platform-server": "2.1.2",
    "@angular/router": "^3.1.2",
    "@types/core-js": "^0.9.34",
    "@types/node": "^6.0.41",
    "core-js": "^2.4.1",
    "reflect-metadata": "^0.1.8",
    "rxjs": "5.0.0-beta.12",
    "zone.js": "0.6.26"
  },
  "devDependencies": {
    "@types/webpack": "^1.12.35",
    "@types/webpack-dev-server": "^1.12.2",
    "angular2-template-loader": "^0.6.0",
    "awesome-typescript-loader": "^2.2.4",
    "cross-env": "^3.1.3",
    "css-loader": "^0.25.0",
    "gzip-size": "^3.0.0",
    "html-webpack-plugin": "^2.24.1",
    "node-sass": "^3.11.2",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.0.2",
    "source-map-explorer": "^1.3.3",
    "style-loader": "^0.13.1",
    "to-string-loader": "^1.1.5",
    "typescript": "2.0.6",
    "webpack": "^2.1.0-beta.25",
    "webpack-dev-server": "^2.1.0-beta.7"
  }
}
